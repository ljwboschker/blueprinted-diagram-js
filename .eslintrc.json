{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "max-len": ["warn", { "code": 150 }],
        "no-use-before-define": "off",
        "class-methods-use-this": "off"
    }
}
